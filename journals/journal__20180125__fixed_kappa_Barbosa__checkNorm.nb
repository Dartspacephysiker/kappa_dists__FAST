Cell[BoxData[{
 RowBox[{
  RowBox[{"normKFull", "[", 
   RowBox[{"V_", ",", "kappa_"}], "]"}], ":=", 
  RowBox[{
   FractionBox["1", 
    RowBox[{
     SuperscriptBox["\[Pi]", 
      RowBox[{"3", "/", "2"}]], 
     SuperscriptBox[
      RowBox[{"(", 
       RowBox[{"2", "-", 
        FractionBox["3", "kappa"]}], ")"}], 
      RowBox[{"3", "/", "2"}]]}]], 
   SuperscriptBox[
    RowBox[{"(", 
     RowBox[{
      RowBox[{
       FractionBox[
        SuperscriptBox["kappa", 
         RowBox[{"3", "/", "2"}]], "2"], 
       FractionBox[
        RowBox[{"Gamma", "[", 
         RowBox[{"kappa", "-", 
          RowBox[{"1", "/", "2"}]}], "]"}], 
        RowBox[{"Gamma", "[", 
         RowBox[{"kappa", "+", "1"}], "]"}]]}], "+", 
      RowBox[{
       FractionBox["V", 
        RowBox[{
         SuperscriptBox[
          RowBox[{"(", 
           RowBox[{"2", "-", 
            FractionBox["3", "kappa"]}], ")"}], 
          RowBox[{"1", "/", "2"}]], 
         SuperscriptBox["\[Pi]", 
          RowBox[{"1", "/", "2"}]]}]], 
       RowBox[{"Hypergeometric2F1", "[", 
        RowBox[{
         FractionBox["1", "2"], ",", "kappa", ",", 
         FractionBox["3", "2"], ",", 
         RowBox[{"-", 
          FractionBox[
           SuperscriptBox["V", "2"], 
           RowBox[{"(", 
            RowBox[{
             RowBox[{"2", "kappa"}], " ", "-", "3"}], ")"}]]}]}], "]"}]}]}], 
     ")"}], 
    RowBox[{"-", "1"}]]}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"barbKappaFull", "[", 
   RowBox[{"vpar_", ",", "vperp_", ",", "V_", ",", "kappa_", ",", "alpha_"}], 
   "]"}], ":=", 
  RowBox[{
   SuperscriptBox[
    RowBox[{"(", 
     RowBox[{"1", "+", 
      FractionBox[
       RowBox[{
        SuperscriptBox["vpar", "2"], "+", 
        SuperscriptBox["vperp", "2"], "+", 
        SuperscriptBox["V", "2"], "-", 
        RowBox[{"2", " ", "V", 
         SuperscriptBox[
          RowBox[{"(", 
           RowBox[{
            SuperscriptBox["vpar", "2"], "+", 
            RowBox[{
             SuperscriptBox["vperp", "2"], 
             RowBox[{"(", 
              RowBox[{"1", "-", "alpha"}], ")"}]}]}], ")"}], 
          RowBox[{"1", "/", "2"}]]}]}], 
       RowBox[{"(", 
        RowBox[{
         RowBox[{"2", " ", "kappa"}], " ", "-", " ", "3"}], ")"}]]}], ")"}], 
    RowBox[{"-", 
     RowBox[{"(", 
      RowBox[{"kappa", "+", "1"}], ")"}]}]], 
   RowBox[{"UnitStep", "[", "vpar", "]"}]}]}]}], "Input",
