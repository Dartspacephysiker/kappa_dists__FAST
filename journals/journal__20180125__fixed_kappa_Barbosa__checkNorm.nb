     RowBox[{"Erfc", "[", 
      RowBox[{"-", 
       SqrtBox[
        RowBox[{"deltaPhi", "/", "T"}]]}], "]"}]}]], 
   RowBox[{"Exp", "[", 
    RowBox[{
     RowBox[{"-", "1"}], 
     RowBox[{"(", 
      RowBox[{
       SuperscriptBox["vpar", "2"], "+", 
       SuperscriptBox["vperp", "2"], "+", 
       RowBox[{"deltaPhi", "/", "T"}], "-", 
       RowBox[{"2", 
        SqrtBox[
         RowBox[{"deltaPhi", "/", "T"}]], " ", 
        SuperscriptBox[
         RowBox[{"(", 
          RowBox[{
           SuperscriptBox["vpar", "2"], "+", " ", 
           RowBox[{
            SuperscriptBox["vperp", "2"], " ", 
            RowBox[{"(", 
             RowBox[{"1", " ", "-", " ", "alpha"}], ")"}]}]}], " ", ")"}], 
         RowBox[{"1", "/", "2"}]]}]}], ")"}]}], "]"}], " ", 
   RowBox[{"UnitStep", "[", "vpar", "]"}]}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"barbAsymp", "[", 
   RowBox[{"deltaPhi_", ",", "T_"}], "]"}], ":=", 
  RowBox[{"1", "+", 
   RowBox[{"2", 
    FractionBox[
     SqrtBox[
      RowBox[{"deltaPhi", "/", "T"}]], 
