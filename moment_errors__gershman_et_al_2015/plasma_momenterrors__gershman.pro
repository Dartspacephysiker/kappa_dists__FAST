     moments_errs.R[10 ,11 ]     = (sigma_Hx_Hy^2.0 - sigma_Hy^2.0 - sigma_Hx^2.0)/(2.0*sigma_Hy*sigma_Hx)
     moments_errs.R[11 ,11 ]     = 1.0
     moments_errs.R[11 ,12 ]     = (sigma_Hy_Hz^2.0 - sigma_Hy^2.0 - sigma_Hz^2.0)/(2.0*sigma_Hy*sigma_Hz)
     moments_errs.R[12 ,11 ]     = (sigma_Hy_Hz^2.0 - sigma_Hy^2.0 - sigma_Hz^2.0)/(2.0*sigma_Hy*sigma_Hz)
     moments_errs.R[12 ,0  ]     = (sigma_n_Hz^2.0 - sigma_Hz^2.0 - sigma_n^2.0)/(2.0*sigma_Hz*sigma_n)
     moments_errs.R[0  ,12 ]     = (sigma_n_Hz^2.0 - sigma_Hz^2.0 - sigma_n^2.0)/(2.0*sigma_Hz*sigma_n)
     moments_errs.R[12 ,1  ]     = (sigma_Ux_Hz^2.0 - sigma_Hz^2.0 - sigma_Ux^2.0)/(2.0*sigma_Hz*sigma_Ux)
     moments_errs.R[1  ,12 ]     = (sigma_Ux_Hz^2.0 - sigma_Hz^2.0 - sigma_Ux^2.0)/(2.0*sigma_Hz*sigma_Ux)
     moments_errs.R[12 ,2  ]     = (sigma_Uy_Hz^2.0 - sigma_Hz^2.0 - sigma_Uy^2.0)/(2.0*sigma_Hz*sigma_Uy)
     moments_errs.R[2  ,12 ]     = (sigma_Uy_Hz^2.0 - sigma_Hz^2.0 - sigma_Uy^2.0)/(2.0*sigma_Hz*sigma_Uy)
     moments_errs.R[12 ,3  ]     = (sigma_Uz_Hz^2.0 - sigma_Hz^2.0 - sigma_Uz^2.0)/(2.0*sigma_Hz*sigma_Uz)
     moments_errs.R[3  ,12 ]     = (sigma_Uz_Hz^2.0 - sigma_Hz^2.0 - sigma_Uz^2.0)/(2.0*sigma_Hz*sigma_Uz)

     IF ~KEYWORD_SET(no_pressure_covar_calc) THEN BEGIN

        moments_errs.R[12 ,4  ]  = (sigma_Pxx_Hz^2.0 - sigma_Hz^2.0 - sigma_Pxx^2.0)/(2.0*sigma_Hz*sigma_Pxx)
        moments_errs.R[4  ,12 ]  = (sigma_Pxx_Hz^2.0 - sigma_Hz^2.0 - sigma_Pxx^2.0)/(2.0*sigma_Hz*sigma_Pxx)
        moments_errs.R[12 ,5  ]  = (sigma_Pyy_Hz^2.0 - sigma_Hz^2.0 - sigma_Pyy^2.0)/(2.0*sigma_Hz*sigma_Pyy)
        moments_errs.R[5  ,12 ]  = (sigma_Pyy_Hz^2.0 - sigma_Hz^2.0 - sigma_Pyy^2.0)/(2.0*sigma_Hz*sigma_Pyy)
        moments_errs.R[12 ,6  ]  = (sigma_Pzz_Hz^2.0 - sigma_Hz^2.0 - sigma_Pzz^2.0)/(2.0*sigma_Hz*sigma_Pzz)
        moments_errs.R[6  ,12 ]  = (sigma_Pzz_Hz^2.0 - sigma_Hz^2.0 - sigma_Pzz^2.0)/(2.0*sigma_Hz*sigma_Pzz)
        moments_errs.R[12 ,7  ]  = (sigma_Pxy_Hz^2.0 - sigma_Hz^2.0 - sigma_Pxy^2.0)/(2.0*sigma_Hz*sigma_Pxy)
        moments_errs.R[7  ,12 ]  = (sigma_Pxy_Hz^2.0 - sigma_Hz^2.0 - sigma_Pxy^2.0)/(2.0*sigma_Hz*sigma_Pxy)
        moments_errs.R[12 ,8  ]  = (sigma_Pxz_Hz^2.0 - sigma_Hz^2.0 - sigma_Pxz^2.0)/(2.0*sigma_Hz*sigma_Pxz)
        moments_errs.R[8  ,12 ]  = (sigma_Pxz_Hz^2.0 - sigma_Hz^2.0 - sigma_Pxz^2.0)/(2.0*sigma_Hz*sigma_Pxz)
        moments_errs.R[12 ,9  ]  = (sigma_Pyz_Hz^2.0 - sigma_Hz^2.0 - sigma_Pyz^2.0)/(2.0*sigma_Hz*sigma_Pyz)
        moments_errs.R[9  ,12 ]  = (sigma_Pyz_Hz^2.0 - sigma_Hz^2.0 - sigma_Pyz^2.0)/(2.0*sigma_Hz*sigma_Pyz)

     ENDIF

     moments_errs.R[12 ,10 ]     = (sigma_Hx_Hz^2.0 - sigma_Hz^2.0 - sigma_Hx^2.0)/(2.0*sigma_Hz*sigma_Hx)
     moments_errs.R[10 ,12 ]     = (sigma_Hx_Hz^2.0 - sigma_Hz^2.0 - sigma_Hx^2.0)/(2.0*sigma_Hz*sigma_Hx)
     moments_errs.R[12 ,11 ]     = (sigma_Hy_Hz^2.0 - sigma_Hz^2.0 - sigma_Hy^2.0)/(2.0*sigma_Hz*sigma_Hy)
     moments_errs.R[11 ,12 ]     = (sigma_Hy_Hz^2.0 - sigma_Hz^2.0 - sigma_Hy^2.0)/(2.0*sigma_Hz*sigma_Hy)
     moments_errs.R[12 ,12 ]     = 1.0

  ENDIF

  moments_errs.n                 = ABS(TEMPORARY(sigma_n))
  moments_errs.Ux                = ABS(TEMPORARY(sigma_Ux))
  moments_errs.Uy                = ABS(TEMPORARY(sigma_Uy))
  moments_errs.Uz                = ABS(TEMPORARY(sigma_Uz))

  ;;Sherlock, this calculates RELATIVE errors
  ;; IF KEYWORD_SET(conv_to_cm) THEN BEGIN
  ;;    moments_errs.n             /= 1d6
  ;;    moments_errs.Ux            *= 100.
  ;;    moments_errs.Uy            *= 100.
  ;;    moments_errs.Uz            *= 100.
  ;; ENDIF

  IF ~KEYWORD_SET(no_pressure_covar_calc) THEN BEGIN

     moments_errs.Pxx               = ABS(TEMPORARY(sigma_Pxx))
     moments_errs.Pyy               = ABS(TEMPORARY(sigma_Pyy))
     moments_errs.Pzz               = ABS(TEMPORARY(sigma_Pzz))
     moments_errs.Pxy               = ABS(TEMPORARY(sigma_Pxy))
     moments_errs.Pxz               = ABS(TEMPORARY(sigma_Pxz))
     moments_errs.Pyz               = ABS(TEMPORARY(sigma_Pyz))

  ENDIF

  IF ~KEYWORD_SET(no_heatFlux_covar_calc) THEN BEGIN

     moments_errs.Hx                = ABS(TEMPORARY(sigma_Hx))
     moments_errs.Hy                = ABS(TEMPORARY(sigma_Hy))
     moments_errs.Hz                = ABS(TEMPORARY(sigma_Hz))

  ENDIF
  
  IF KEYWORD_SET(sans_phi) THEN BEGIN

     moments_errs.Uy                = moments_errs.Ux
     moments_errs.Pyy               = moments_errs.Pxx
     moments_errs.Hy                = moments_errs.Hx

  ENDIF

  RETURN,moments_errs

END