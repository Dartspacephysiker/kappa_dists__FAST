  calc_Pzz_Hy   = 0.0
  err_Pzz_Hy    = 0.0

  calc_Pzz_Hz   = 0.0
  err_Pzz_Hz    = 0.0

  calc_Pxy_Pxy  = 0.0
  err_Pxy_Pxy   = 0.0

  calc_Pxy      = 0.0
