;
;  :Description:
;     This function computes relative errors in the moments of a three-dimensional phase space distribution
;           EXAMPLE CALL: errs = sfts00_plasma_momenterrors(f,sigma_f,species,phi,theta,energy)
;
;     This software is free for use and modification with appropriate citation:
;       Gershman, D.J., et al. (2015), The calculation of moment uncertainties from velocity distribution functions with random errors, J.Geophys. Res.: Space Physics
;
;  :Authors:
;     Daniel J. Gershman, John C. Dorelli, Adolfo F.-Vi√±as, and Craig J. Pollock
;
;  :History:
;     10/14/2014 -- Daniel J. Gershman, NASA-GSFC:  initial version submitted to JGR: Space Physics
;     4/7/2015   -- Daniel J. Gershman, NASA-GSFC: revised version submitted to JGR: Space Physics
;     2017/02/21 -- Spencer Hatch, Dartmouth College: Adapting for my own wicked designs
;
;  :Params:
;
;     f : in, type=dblarr(N_en, N_theta, N_phi), phase space density in s^3/m^6, where:
;        N_en    = number of energy bins
;        N_theta = number of elevation bins (angle from Z axis)
;        N_phi   = number of azimuthal bins (angle in the XY plane)
;
;     sigma_f      : in, type=dblarr(N_phi, N_theta, N_en), phase space density Poisson error in s^3/m^6
;
;     species      : in, type=int, species = 0 for electrons, 1 for ions
;
;     phi          : in, type=dblarr(N_phi), azimuthal angle in degrees (increasing with index)
;
;     theta        : in, type=dblarr(N_theta), elevation angle in degrees (increasing with index)
;
;     energy       : in, type=dblarr(N_en), energy in eV (increasing with index)
;
;     moments_errs : out, type=named struct:  'moments', relative error estimates (greater than or equal to 0)
;        struct = {n:  0d0,                              ; number density  - index 0
;                  Ux:  0d0,     $                 ; X component of bulk velocity, index 1
;                  Uy:  0d0,     $                 ; Y component of bulk velocity, index 2
;                  Uz:  0d0,     $                 ; Z component of bulk velocity, index 3
;                  Pxx:  0d0,    $                 ; XX component of pressure tensor, index 4
;                  Pyy:  0d0,    $                 ; YY component of pressure tensor, index 5
;                  Pzz:  0d0,    $                 ; ZZ component of pressure tensor, index 6
;                  Pxy:  0d0,    $                 ; XY component of pressure tensor, index 7
;                  Pxz:  0d0,    $                 ; XZ component of pressure tensor, index 8
;                  Pyz:  0d0,    $                 ; YZ component of pressure tensor, index 9
;                  Hx:  0d0,     $                 ; X component of heat flux, index 10
;                  Hy:  0d0,     $                 ; Y component of heat flux, index 11
;                  Hz:  0d0,     $                 ; Z component of heat flux, index 12
;                  R:  dblarr(13,13)                ; correlation matrix,
;
;                takes values between -1 and 1 that represent the statistical correlation between two moments,
;
;                values are ordered by indices above
;
;                                                                                                 ;
;                 }
;-
FUNCTION PLASMA_MOMENTERRORS__GERSHMAN,f,sigma_f,species,energy,theta,phi, $
                                       DENERGY=dEnergy, $
                                       DTHETA=dTheta, $
                                       ;; NO_PRESSURE_COVAR_CALC=no_pressure_covar_calc, $
                                       ;; NO_HEATFLUX_COVAR_CALC=no_heatFlux_covar_calc
                                       PRESSURE_COVAR_CALC=pressure_covar_calc, $
                                       HEATFLUX_COVAR_CALC=heatFlux_covar_calc, $
                                       SANS_PHI=sans_phi, $
                                       SANS_OFFDIAG=sans_offDiag;; , $
                                       ;; CONV_TO_CM=conv_to_cm ;;What were you thinking? This calculates RELATIVE errors

  COMPILE_OPT idl2

  no_pressure_covar_calc  = ~KEYWORD_SET(pressure_covar_calc)
  no_heatFlux_covar_calc  = ~KEYWORD_SET(heatFlux_covar_calc)

  revEn                   = 0
  revTheta                = 0
  revPhi                  = 0
  CHECK_SORTED,energy,en_is_sorted,/QUIET
  CHECK_SORTED,theta,theta_is_sorted,/QUIET
  IF ~(en_is_sorted OR KEYWORD_SET(dEnergy)) THEN BEGIN
     CHECK_SORTED,REVERSE(energy),en_is_sorted,/QUIET
     revEn                =  en_is_sorted

     IF revEn THEN BEGIN
        energy            = REVERSE(energy   )
        f                 = REVERSE(f      ,1)
        sigma_f           = REVERSE(sigma_f,1)
        IF KEYWORD_SET(dEnergy) THEN BEGIN
           dEnergy        = REVERSE(dEnergy)
        ENDIF
     ENDIF ELSE BEGIN
        STOP
     ENDELSE
  ENDIF
  IF ~(theta_is_sorted OR KEYWORD_SET(dTheta)) THEN BEGIN
     CHECK_SORTED,REVERSE(theta),theta_is_sorted,/QUIET
     revTheta             =  theta_is_sorted

     IF revTheta THEN BEGIN
        theta             = REVERSE(theta    )
        f                 = REVERSE(f      ,2)
        sigma_f           = REVERSE(sigma_f,2)
        IF KEYWORD_SET(dEnergy) THEN BEGIN
           dTheta         = REVERSE(dTheta)
        ENDIF
     ENDIF ELSE BEGIN
        STOP
        sort_i            = SORT(theta)
        theta             = theta[sort_i]
        dTheta            = dTheta[sort_i]
        f                 = f[*,sort_i]
        sigma_f           = f[*,sort_i]
     ENDELSE
  ENDIF

  IF ~KEYWORD_SET(sans_phi) THEN BEGIN

     CHECK_SORTED,phi,phi_is_sorted,/QUIET
     IF ~phi_is_sorted THEN BEGIN
        CHECK_SORTED,REVERSE(phi),phi_is_sorted,/QUIET
        revPhi            =  phi_is_sorted

        IF revPhi THEN BEGIN
           phi            = REVERSE(phi      )
           f              = REVERSE(f      ,3)
           sigma_f        = REVERSE(sigma_f,3)
        ENDIF ELSE BEGIN
           STOP
        ENDELSE
     ENDIF
     
  ENDIF

  energy_1eV              = 1.6022d-19 ; kinetic energy of a single electron volt (J)
  kB                      = 1.3807d-23 ; Boltzmann's constant
  CASE species OF
     0:  mass             = 9.1094d-31 ; mass of electron (kg)
     1:  mass             = 1.6726d-27 ; mass of a proton (kg)
  ENDCASE

  vs                      = SQRT(2*energy*energy_1eV/mass) ; convert energy (eV) array to velocity (m/s)
  IF KEYWORD_SET(dEnergy) THEN BEGIN
     dVel                 = SQRT(2*dEnergy*energy_1eV/mass) ; convert energy (eV) array to velocity (m/s)
  ENDIF ELSE BEGIN
     dVel                 = [vs[0],vs[1:-1]-vs[0:-2]]
     ;; IF vii GT 0 THEN BEGIN
     ;;    dv = vs[vii]-vs[vii-1]
     ;; ENDIF ELSE BEGIN
     ;;    dv = vs[vii]
     ;; ENDELSE
  ;; ENDELSE
  ENDELSE

  IF KEYWORD_SET(dTheta) THEN BEGIN
     dTh    = dTheta*!DTOR
  ENDIF ELSE BEGIN
     dTh    = (theta[tii]-theta[tii-1])*!DTOR
  ENDELSE

  ;;Number of bins in each dimension
  Nen                     = N_ELEMENTS(energy)
  Ntheta                  = N_ELEMENTS(theta)
  Nphi                    = KEYWORD_SET(sans_phi) ? 0 : N_ELEMENTS(phi)

  N_R                     = 4 + (KEYWORD_SET(no_pressure_covar_calc) ? 0 : 6) + (KEYWORD_SET(no_heatFlux_covar_calc) ? 0 : 3)
  moments_errs            = {n   :  0d0, $                                ; number density  - index 0
                             Ux  :  0d0, $                                ; X component of bulk velocity, index 1
                             Uy  :  0d0, $                                ; Y component of bulk velocity, index 2
                             Uz  :  0d0, $                                ; Z component of bulk velocity, index 3
                             Pxx :  0d0, $                                ; XX component of pressure tensor, index 4
                             Pyy :  0d0, $                                ; YY component of pressure tensor, index 5
                             Pzz :  0d0, $                                ; ZZ component of pressure tensor, index 6
                             Pxy :  0d0, $                                ; XY component of pressure tensor, index 7
                             Pxz :  0d0, $                                ; XZ component of pressure tensor, index 8
                             Pyz :  0d0, $                                ; YZ component of pressure tensor, index 9
                             Hx  :  0d0, $                                ; Y component of heat flux, index 10
                             Hy  :  0d0, $                                ; Y component of heat flux, index 11
                             Hz  :  0d0, $                                ; Z component of heat flux, index 12
                             R   :  MAKE_ARRAY(N_R,N_R,/DOUBLE,VALUE=0.0D)} ; correlation matrix

  CASE 1 OF
     KEYWORD_SET(sans_phi): BEGIN

        ;; create arrays for fast Riemann-sum integration
        v0s    = MAKE_ARRAY(Nen,Ntheta,/DOUBLE,VALUE=1.D) ; array of 1s
        vxs    = MAKE_ARRAY(Nen,Ntheta,/DOUBLE,VALUE=0.D) ; vx value at each bin center
        vys    = MAKE_ARRAY(Nen,Ntheta,/DOUBLE,VALUE=0.D) ; vy value at each bin center
        vzs    = MAKE_ARRAY(Nen,Ntheta,/DOUBLE,VALUE=0.D) ; vz value at each bin center
        vms    = MAKE_ARRAY(Nen,Ntheta,/DOUBLE,VALUE=0.D) ; sqrt(vx^2+vy^2+vz^2) value at each bin center
        d3vs   = MAKE_ARRAY(Nen,Ntheta,/DOUBLE,VALUE=0.D) ; phase space volume of each bin
        dOmega = MAKE_ARRAY(Nen,Ntheta,/DOUBLE,VALUE=0.D) ; solid angle

        ;; cphi   = 0.D
        ;; cphi              = !PI/4.

        FOR tii=0,Ntheta-1 DO BEGIN
           FOR vii=0,Nen-1 DO BEGIN

              ;; cv = vs[vii]
              ;; ctheta = theta[tii]*!pi/180.0

              ;; vxs[vii,tii] = vs[vii]*SIN(theta[tii]*!DTOR)*COS(cphi)
              ;; vys[vii,tii] = vs[vii]*SIN(theta[tii]*!DTOR)*SIN(cphi)

              ;;Stop kidding yourself. There's no need to waste time with vy
              vxs[vii,tii] = vs[vii]*SIN(theta[tii]*!DTOR)
              ;; vys[vii,tii] = vs[vii]*SIN(theta[tii]*!DTOR)*SIN(cphi)

              ;; vxs[vii,tii] = vs[vii]*SIN(theta[tii]*!DTOR)
              vzs[vii,tii] = vs[vii]*COS(theta[tii]*!DTOR)
              vms[vii,tii] = vs[vii]


              ;; form phase space volumes using left-Riemann sum (assumes values increase with index)
              ;; IF vii GT 0 THEN BEGIN
              ;;    dv = vs[vii]-vs[vii-1]
              ;; ENDIF ELSE BEGIN
              ;;    dv = vs[vii]
              ;; ENDELSE
              

              ;; IF tii GT 0 THEN BEGIN
              ;;    dtheta = (theta[tii]-theta[tii-1])*!pi/180.0
              ;; ENDIF ELSE BEGIN
              ;;    dtheta = theta[tii]*!pi/180.0
              ;; ENDELSE

              d3vs[vii,tii]   = (vs[vii])^2*SIN(theta[tii]*!DTOR)*dTh[tii]*dVel[vii]
              dOmega[vii,tii] = SIN(theta[tii]*!DTOR)*dTh[tii]
           ENDFOR
        ENDFOR

        ;; vys     = vxs

     END
     ELSE: BEGIN

        Nphi    = N_ELEMENTS(phi)

        ;; create arrays for fast Riemann-sum integration
        v0s     = MAKE_ARRAY(Nen,Ntheta,Nphi,/DOUBLE,VALUE=1.D) ; array of 1s
        vxs     = MAKE_ARRAY(Nen,Ntheta,Nphi,/DOUBLE,VALUE=0.D) ; vx value at each bin center
        vys     = MAKE_ARRAY(Nen,Ntheta,Nphi,/DOUBLE,VALUE=0.D) ; vy value at each bin center
        vzs     = MAKE_ARRAY(Nen,Ntheta,Nphi,/DOUBLE,VALUE=0.D) ; vz value at each bin center
        vms     = MAKE_ARRAY(Nen,Ntheta,Nphi,/DOUBLE,VALUE=0.D) ; sqrt(vx^2+vy^2+vz^2) value at each bin center
        d3vs    = MAKE_ARRAY(Nen,Ntheta,Nphi,/DOUBLE,VALUE=0.D) ; phase space volume of each bin

        FOR pii=0,Nphi-1 DO BEGIN
           FOR tii=0,Ntheta-1 DO BEGIN
              FOR vii=0,Nen-1 DO BEGIN

                 ;; cv = vs[vii]
                 ;; ctheta = theta[tii]*!pi/180.0
                 cphi = phi[pii]*!pi/180.0

                 vxs[vii,tii,pii] = vs[vii]*SIN(theta[tii]*!DTOR)*COS(cphi)
                 vys[vii,tii,pii] = vs[vii]*SIN(theta[tii]*!DTOR)*SIN(cphi)
                 vzs[vii,tii,pii] = vs[vii]*COS(theta[tii]*!DTOR)
                 vms[vii,tii,pii] = vs[vii]


                 ;; form phase space volumes using left-Riemann sum (assumes values increase with index)
                 IF vii GT 0 THEN BEGIN
                    dv = vs[vii]-vs[vii-1]
                 ENDIF ELSE BEGIN
                    dv = vs[vii]
                 ENDELSE

                 IF tii GT 0 THEN BEGIN
                    dtheta = (theta[tii]-theta[tii-1])*!pi/180.0
                 ENDIF ELSE BEGIN
                    dtheta = theta[tii]*!pi/180.0
                 ENDELSE

                 IF pii GT 0 THEN BEGIN
                    dphi = (phi[pii]-phi[pii-1])*!pi/180.0
                 ENDIF ELSE BEGIN
                    dphi = (phi[Nphi-1]-phi[pii])*!pi/180.0 ; enable wrapping in azimuth
                 ENDELSE

                 IF dphi LT 0 THEN BEGIN
                    dphi = dphi + 2.0*!pi
                 ENDIF

                 d3vs[vii,tii,pii] = vs[vii]^2*SIN(theta[tii]*!DTOR)*dtheta*dphi*dv

              ENDFOR
           ENDFOR
        ENDFOR

     END
  ENDCASE

  ;;Calculated counts in these variables (yes, fractional counts)
  n_v0s         = 0.0
  n_vxs         = 0.0
  n_vys         = 0.0
  n_vzs         = 0.0
  n_vxstvxs     = 0.0
  n_vxstvys     = 0.0
  n_vxstvzs     = 0.0
  n_vystvys     = 0.0
  n_vystvzs     = 0.0
  n_vzstvzs     = 0.0
  n_vxstvxstvxs = 0.0
  n_vxstvxstvys = 0.0
  n_vxstvxstvzs = 0.0
  n_vxstvystvys = 0.0
  n_vxstvzstvzs = 0.0
  n_vystvystvys = 0.0
  n_vystvystvzs = 0.0
  n_vystvzstvzs = 0.0
  n_vzstvzstvzs = 0.0


  ;;Initialize vars
  calc_n_n      = 0.0
  err_n_n       = 0.0

  calc_n        = 0.0
  err_n         = 0.0

  calc_n_Ux     = 0.0
  err_n_Ux      = 0.0

  calc_n_Uy     = 0.0
  err_n_Uy      = 0.0

  calc_n_Uz     = 0.0
  err_n_Uz      = 0.0

  calc_n_Pxx    = 0.0
  err_n_Pxx     = 0.0

  calc_n_Pyy    = 0.0
  err_n_Pyy     = 0.0

  calc_n_Pzz    = 0.0
  err_n_Pzz     = 0.0

  calc_n_Pxy    = 0.0
  err_n_Pxy     = 0.0

  calc_n_Pxz    = 0.0
  err_n_Pxz     = 0.0

  calc_n_Pyz    = 0.0
  err_n_Pyz     = 0.0

  calc_n_Hx     = 0.0
  err_n_Hx      = 0.0

  calc_n_Hy     = 0.0
  err_n_Hy      = 0.0

  calc_n_Hz     = 0.0
  err_n_Hz      = 0.0

  calc_Ux_Ux    = 0.0
  err_Ux_Ux     = 0.0

  calc_Ux       = 0.0
  err_Ux        = 0.0

  calc_Ux_Uy    = 0.0
  err_Ux_Uy     = 0.0

  calc_Ux_Uz    = 0.0
  err_Ux_Uz     = 0.0

  calc_Ux_Pxx   = 0.0
  err_Ux_Pxx    = 0.0

  calc_Ux_Pyy   = 0.0
  err_Ux_Pyy    = 0.0

  calc_Ux_Pzz   = 0.0
  err_Ux_Pzz    = 0.0

  calc_Ux_Pxy   = 0.0
  err_Ux_Pxy    = 0.0

  calc_Ux_Pxz   = 0.0
  err_Ux_Pxz    = 0.0

  calc_Ux_Pyz   = 0.0
  err_Ux_Pyz    = 0.0

  calc_Ux_Hx    = 0.0
  err_Ux_Hx     = 0.0

  calc_Ux_Hy    = 0.0
  err_Ux_Hy     = 0.0

  calc_Ux_Hz    = 0.0
  err_Ux_Hz     = 0.0

  calc_Uy_Uy    = 0.0
  err_Uy_Uy     = 0.0

  calc_Uy       = 0.0
  err_Uy        = 0.0

  calc_Uy_Uz    = 0.0
  err_Uy_Uz     = 0.0

  calc_Uy_Pxx   = 0.0
  err_Uy_Pxx    = 0.0

  calc_Uy_Pyy   = 0.0
  err_Uy_Pyy    = 0.0

  calc_Uy_Pzz   = 0.0
  err_Uy_Pzz    = 0.0

  calc_Uy_Pxy   = 0.0
  err_Uy_Pxy    = 0.0

  calc_Uy_Pxz   = 0.0
  err_Uy_Pxz    = 0.0

  calc_Uy_Pyz   = 0.0
  err_Uy_Pyz    = 0.0

  calc_Uy_Hx    = 0.0
  err_Uy_Hx     = 0.0

  calc_Uy_Hy    = 0.0
  err_Uy_Hy     = 0.0

  calc_Uy_Hz    = 0.0
  err_Uy_Hz     = 0.0

  calc_Uz_Uz    = 0.0
  err_Uz_Uz     = 0.0

  calc_Uz       = 0.0
  err_Uz        = 0.0

  calc_Uz_Pxx   = 0.0
  err_Uz_Pxx    = 0.0

  calc_Uz_Pyy   = 0.0
  err_Uz_Pyy    = 0.0

  calc_Uz_Pzz   = 0.0
  err_Uz_Pzz    = 0.0

  calc_Uz_Pxy   = 0.0
  err_Uz_Pxy    = 0.0

  calc_Uz_Pxz   = 0.0
  err_Uz_Pxz    = 0.0

  calc_Uz_Pyz   = 0.0
  err_Uz_Pyz    = 0.0

  calc_Uz_Hx    = 0.0
  err_Uz_Hx     = 0.0

  calc_Uz_Hy    = 0.0
  err_Uz_Hy     = 0.0

  calc_Uz_Hz    = 0.0
  err_Uz_Hz     = 0.0

  calc_Pxx_Pxx  = 0.0
  err_Pxx_Pxx   = 0.0

  calc_Pxx      = 0.0
  err_Pxx       = 0.0

  calc_Pxx_Pyy  = 0.0
  err_Pxx_Pyy   = 0.0

  calc_Pxx_Pzz  = 0.0
  err_Pxx_Pzz   = 0.0

  calc_Pxx_Pxy  = 0.0
  err_Pxx_Pxy   = 0.0

  calc_Pxx_Pxz  = 0.0
  err_Pxx_Pxz   = 0.0

  calc_Pxx_Pyz  = 0.0
  err_Pxx_Pyz   = 0.0

  calc_Pxx_Hx   = 0.0
  err_Pxx_Hx    = 0.0

  calc_Pxx_Hy   = 0.0
  err_Pxx_Hy    = 0.0

  calc_Pxx_Hz   = 0.0
  err_Pxx_Hz    = 0.0

  calc_Pyy_Pyy  = 0.0
  err_Pyy_Pyy   = 0.0

  calc_Pyy      = 0.0
  err_Pyy       = 0.0

  calc_Pyy_Pzz  = 0.0
  err_Pyy_Pzz   = 0.0

  calc_Pyy_Pxy  = 0.0
  err_Pyy_Pxy   = 0.0

  calc_Pyy_Pxz  = 0.0
  err_Pyy_Pxz   = 0.0

  calc_Pyy_Pyz  = 0.0
  err_Pyy_Pyz   = 0.0

  calc_Pyy_Hx   = 0.0
  err_Pyy_Hx    = 0.0

  calc_Pyy_Hy   = 0.0
  err_Pyy_Hy    = 0.0

  calc_Pyy_Hz   = 0.0
  err_Pyy_Hz    = 0.0

  calc_Pzz_Pzz  = 0.0
  err_Pzz_Pzz   = 0.0

  calc_Pzz      = 0.0
  err_Pzz       = 0.0

  calc_Pzz_Pxy  = 0.0
  err_Pzz_Pxy   = 0.0

  calc_Pzz_Pxz  = 0.0
  err_Pzz_Pxz   = 0.0

  calc_Pzz_Pyz  = 0.0
  err_Pzz_Pyz   = 0.0

  calc_Pzz_Hx   = 0.0
