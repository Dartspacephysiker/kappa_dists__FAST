  calc_Pxy_Pxz  = 0.0
  err_Pxy_Pxz   = 0.0

  calc_Pxy_Pyz  = 0.0
  err_Pxy_Pyz   = 0.0

  calc_Pxy_Hx   = 0.0
  err_Pxy_Hx    = 0.0

  calc_Pxy_Hy   = 0.0
  err_Pxy_Hy    = 0.0

  calc_Pxy_Hz   = 0.0
  err_Pxy_Hz    = 0.0

  calc_Pxz_Pxz  = 0.0
  err_Pxz_Pxz   = 0.0

  calc_Pxz      = 0.0
  err_Pxz       = 0.0

  calc_Pxz_Pyz  = 0.0
  err_Pxz_Pyz   = 0.0

  calc_Pxz_Hx   = 0.0
  err_Pxz_Hx    = 0.0

  calc_Pxz_Hy   = 0.0
  err_Pxz_Hy    = 0.0

  calc_Pxz_Hz   = 0.0
  err_Pxz_Hz    = 0.0

  calc_Pyz_Pyz  = 0.0
  err_Pyz_Pyz   = 0.0

  calc_Pyz      = 0.0
  err_Pyz       = 0.0

  calc_Pyz_Hx   = 0.0
  err_Pyz_Hx    = 0.0

  calc_Pyz_Hy   = 0.0
  err_Pyz_Hy    = 0.0

  calc_Pyz_Hz   = 0.0
  err_Pyz_Hz    = 0.0

  calc_Hx_Hx    = 0.0
  err_Hx_Hx     = 0.0

  calc_Hx       = 0.0
  err_Hx        = 0.0

  calc_Hx_Hy    = 0.0
  err_Hx_Hy     = 0.0

  calc_Hx_Hz    = 0.0
  err_Hx_Hz     = 0.0

  calc_Hy_Hy    = 0.0
  err_Hy_Hy     = 0.0

  calc_Hy       = 0.0
  err_Hy        = 0.0

  calc_Hy_Hz    = 0.0
  err_Hy_Hz     = 0.0

  calc_Hz_Hz    = 0.0
  err_Hz_Hz     = 0.0

  calc_Hz       = 0.0
  err_Hz        = 0.0


  ;;Calculate counts here
  n_v0s         = TOTAL(f*v0s*d3vs)
  n_vxs         = TOTAL(f*vxs*d3vs)
  n_vys         = TOTAL(f*vys*d3vs)
  n_vzs         = TOTAL(f*vzs*d3vs)
  n_vxstvxs     = TOTAL(f*vxs*vxs*d3vs)
  n_vxstvys     = TOTAL(f*vxs*vys*d3vs)
  n_vxstvzs     = TOTAL(f*vxs*vzs*d3vs)
  n_vystvys     = TOTAL(f*vys*vys*d3vs)
  n_vystvzs     = TOTAL(f*vys*vzs*d3vs)
  n_vzstvzs     = TOTAL(f*vzs*vzs*d3vs)
  n_vxstvxstvxs = TOTAL(f*vxs*vxs*vxs*d3vs)
  n_vxstvxstvys = TOTAL(f*vxs*vxs*vys*d3vs)
  n_vxstvxstvzs = TOTAL(f*vxs*vxs*vzs*d3vs)
  n_vxstvystvys = TOTAL(f*vxs*vys*vys*d3vs)
  n_vxstvzstvzs = TOTAL(f*vxs*vzs*vzs*d3vs)
  n_vystvystvys = TOTAL(f*vys*vys*vys*d3vs)
